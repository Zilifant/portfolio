{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "sass": "./node_modules/.bin/node-sass src/styles/global.scss public/global.css",
    "sassdev": "./node_modules/.bin/node-sass src/styles/global.scss public/global.css -w",
    "build": "npm run sass && rollup -c",
    "dev": "sassdev -w && rollup -c -w",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.9",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.9.8"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
