{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "sass": "./node_modules/.bin/node-sass src/styles -o public",
    "sassdev": "./node_modules/.bin/node-sass src/styles -o public -w",
    "prebuild": "npm run sass",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --no-clear --host --port 5555",
    "predeploy": "npm run build",
    "deploy": "cd public && vercel deploy && vercel --prod && cd .."
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "autoprefixer": "^10.3.7",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.9",
    "postcss-cli": "^9.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^4.9.8"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  }
}
